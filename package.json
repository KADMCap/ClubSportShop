    "@mui/material": "^5.11.16",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "classnames": "^2.3.2",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
